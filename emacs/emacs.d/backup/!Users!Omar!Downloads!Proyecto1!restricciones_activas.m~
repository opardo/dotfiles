% Omar Trejo Navarro, 119711
% Natalia Orozco Urquijo, 111008

function [ W ] = Rest_Activas(A, x, b)
% Calcula el vector de restricciones activas W en x

% In:
% - A: matriz en R^mxn de rango m con m<=n.
% - b: vector en R^m.
% - x: solución factible de Ax=b

W=[];
[m,n]=size(A);

for i = 1:m
	aix = A(i,:)*x;
	bi = b(i);
	if aix == bi
		W=[W;i];
	end
end


end

