clear all
close all
clc

%% Parametros

alpha = 0.35;
delta = 0.06;
A     = 10;
tau   = 0;
maxit = 1000;
T     = 100;
kss   = ((A*beta*alpha)/(1 - (1 - delta)*beta))^(1/(1 - alpha));
k0    = (1/2)*kss;
malla = 500;
crit  = 1e-3;

%% Inciso 1

%% Parte 1

beta  = 0.99;
tic
[kt_vf, yt_vf, it_vf, ct_vf] = gs(alpha, ...
                                  beta, ...
                                  delta, ...
                                  A, ...
                                  maxit, ...
                                  crit, ...
                                  T, ...
                                  kss, ...
                                  k0, ...
                                  malla);
toc

rt_vf = alpha.*A*kt_vf.^(alpha - 1); % Tasa de interés
wt_vf = (1-alpha).*A*kt_vf.^alpha;   % Salarios

grafica_P3P4(kt_vf, yt_vf, it_vf, ct_vf, rt_vf, wt_vf, T)

%% Parte 2

beta  = 0.98;
tic
[kt_vf, yt_vf, it_vf, ct_vf] = gs(alpha, ...
                                  beta, ...
                                  delta, ...
                                  A, ...
                                  maxit, ...
                                  crit, ...
                                  T, ...
                                  kss, ...
                                  k0, ...
                                  malla);
toc

rt_vf = alpha.*A*kt_vf.^(alpha - 1); % Tasa de interés
wt_vf = (1-alpha).*A*kt_vf.^alpha;   % Salarios

grafica_P3P4(kt_vf, yt_vf, it_vf, ct_vf, rt_vf, wt_vf, T)

