function [x, lambda] = pcrango(A, Q, c, b)
    %
    %
    %
    n = length(c);
    m = length(b);
    Q1 = inv(Q);
    A1 = A*Q1*A';
    g = -(A*Q1*c + b);
    % Sistema lineal A1*lambda - g;
    % A1 es sim√©trica positiva definida.
    L = chol(A);
    lambda = L'\(L\g);
    x = -Q1*(c + A'*lambda);
end
